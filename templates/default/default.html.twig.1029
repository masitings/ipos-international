                                        <div class="events-box__bottom__top">
                                            <div class="categories-tag">{{ upcoming.eventType }}</div>
                                            <h3 class="events-box-title m-t-10 m-b-30">{{ upcoming.title }}</h3>
                                        </div>
                                        <div class="events-box__bottom__bottom">

                                            {% for date in upcoming.planing %}
                                                {% for time in date.teachingArrangement %}
                                                    <div class="d-flex m-b-5">
                                                        <div class="events-box__bottom__bottom__icon m-r-10">
                                                            <svg viewBox="0 0 10 9" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                                <path d="M5 8.5C7.20914 8.5 9 6.70914 9 4.5C9 2.29086 7.20914 0.5 5 0.5C2.79086 0.5 1 2.29086 1 4.5C1 6.70914 2.79086 8.5 5 8.5Z" stroke="#470A68" stroke-linecap="round" stroke-linejoin="round"/>
                                                                <path d="M6.37818 5.8778L4.86707 4.94447V2.41113" stroke="#470A68" stroke-linecap="round" stroke-linejoin="round"/>
                                                            </svg>

                                                        </div>
                                                        <span>{{ time.startTime.data | date('H A') }} - {{ time.lastTime.data | date('H A') }}</span>

                                                    </div>
                                                {% endfor %}

                                            {% endfor %}
                                            <div class="d-flex m-b-5">
                                                <div class="events-box__bottom__bottom__icon m-r-10"><svg viewBox="1 1 10 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M6 7V7C5.1715 7 4.5 6.3285 4.5 5.5V5.5C4.5 4.6715 5.1715 4 6 4V4C6.8285 4 7.5 4.6715 7.5 5.5V5.5C7.5 6.3285 6.8285 7 6 7Z" stroke="#470A68" stroke-linecap="round" stroke-linejoin="round"/>
                                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M6 11C6 11 2.5 8.125 2.5 5.5C2.5 3.567 4.067 2 6 2C7.933 2 9.5 3.567 9.5 5.5C9.5 8.125 6 11 6 11Z" stroke="#470A68" stroke-linecap="round" stroke-linejoin="round"/>
                                                    </svg>
                                                </div><span>{{ upcoming.venue }}</span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="events-box__right">
                                        {% for date in upcoming.planing %}
                                            <div class="event-date">
                                                {% if date.lastDate is not empty %}
                                                    <div>{{ date.startDate | date('d') }} - {{ date.lastDate | date('d') }}
                                                    </div>
                                                    <div>{{ date.lastDate | date('M') }}</div>

                                                {% else %}
                                                    <div>{{ date.startDate | date('d') }}</div>
                                                    <div>{{ date.startDate | date('M') }}</div>
                                                {% endif %}

                                            </div>
                                        {% endfor %}
                                        <div class="event-share">
                                            <svg width="21" height="20" viewBox="0 0 21 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                <path d="M11.7749 6.2666L8.3916 8.79994" stroke="#777777" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
                                                <path d="M14.1706 3.86804C14.8319 4.52932 14.8319 5.60149 14.1706 6.26277C13.5093 6.92406 12.4372 6.92406 11.7759 6.26277C11.1146 5.60148 11.1146 4.52932 11.7759 3.86804C12.4372 3.20675 13.5093 3.20675 14.1706 3.86804" stroke="#777777" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
                                                <path d="M11.7749 13.7326L8.3916 11.1992" stroke="#777777" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
                                                <path d="M14.1706 13.7362C14.8319 14.3975 14.8319 15.4696 14.1706 16.1309C13.5093 16.7922 12.4372 16.7922 11.7759 16.1309C11.1146 15.4696 11.1146 14.3975 11.7759 13.7362C12.4372 13.0749 13.5093 13.0749 14.1706 13.7362" stroke="#777777" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
                                                <path d="M8.38785 8.80309C9.04849 9.46373 9.04849 10.5348 8.38785 11.1955C7.72722 11.8561 6.65611 11.8561 5.99548 11.1955C5.33484 10.5348 5.33484 9.46373 5.99548 8.80309C6.65611 8.14246 7.72722 8.14246 8.38785 8.80309" stroke="#777777" stroke-width="1.25" stroke-linecap="round" stroke-linejoin="round"/>
                                            </svg>

                                        </div>

                                    </div>

                                </a>
                            {% endfor %}
                        </div>
                    </div>
			{{ pimcore_link('upeventLink',{
				'class' : 'btn btn__hollow mx-auto'
			}) }}
                </div>
            </div>
        {% endif %}

    </div>

    <div class="darkCircle">VIEW</div>
    <div class="test"></div>

{% endblock %}

