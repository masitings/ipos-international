{% extends 'layouts/layout.html.twig' %}

{% block content %}

    <section class="sec-horizon">
        <div class="bg-cover-horizon">
            <h2 class="bg-cover-horizon__title">Ideas Today, <br class="d-block d-lg-none">Assets Tomorrow.</h2>
            <div class="scroll-indicator">SCROLL</div><a class="see-what-new" href="#WhatsNew">What's New</a>
        </div>
    </section>
    <section class="sec-what-new p-t-100" id="WhatsNew">
        <div class="container">
            <h2 class="ipos-title text-center m-b-30">What's New</h2>
            <div class="sec-what-new__row row lg-p-l-60 lg-p-r-20">
                <div class="col-lg-7 m-b-50">
                    {% if list.analytics is not empty %}
                        <div class="new-box lg-m-r-15">
                            <div class="new-box__upper">

                                {{ list.analytics.coverImage.thumbnail('coverImg').html({imgAttributes:{class:'img-fluid w-100'}}) | raw }}
                            </div>
                            <div class="new-box__bottom">
                                <div class="new-box__bottom__top">
                                    <div class="categories-tag">{{ list.analytics.resourceType }}</div>
                                    <h3 class="new-box-title m-t-10">{{ list.analytics.title }}</h3>
                                </div>
                                <div class="new-box__bottom__bottom">
                                    <a href="{{ list.analytics }}_{{ list.analytics.getId() }}">Find out more</a>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                </div>
                <div class="col-lg-4">
                    {% if  list.news.data is not null %}

                        <div class="new-box new-box--higher lg-m-l-15 lg-m-t-160">
                            <div class="new-box__upper">
                                {{ list.news.data.coverImage.thumbnail('coverImg').html({imgAttributes:{class:'img-fluid w-100'}}) | raw }}
                            </div>
                            <div class="new-box__bottom">
                                <div class="new-box__bottom__top">
                                    <div class="categories-tag">{{ list.news.type }}</div>
                                    <h3 class="new-box-title m-t-10">{{ list.news.data.title }}</h3>
                                </div>

                                <div class="new-box__bottom__bottom">
                                    <a href="/News{{ list.news.data }}_{{ list.news.data.getId() }}">Find out more</a>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                </div>
                <div class="col-lg-5 offset-lg-1">
                    {% if list.article is not empty %}
                        <div class="new-box new-box--empty lg-m-r-15">

                            <div class="new-box__bottom">
                                <div class="new-box__bottom__top">
                                    <div class="categories-tag">{{ list.article.resourceType }}</div>
                                    <h3 class="new-box-title m-t-10">{{ list.article.title }}</h3>
                                </div>
                                <div class="new-box__bottom__bottom">
                                    <a href="{{ list.article }}_{{ list.article.getId() }}">Find out more</a>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                </div>
                 <div class="col-lg-6">
                    {% if list.course is not empty %}
                        <div class="new-box new-box--row lg-m-l-15 lg-m-t-90">
                            <div class="new-box__upper">
{#                                <img class="img-fluid" src="/static/images/new-thumb.55b8caa.png" alt="">#}
                                {{ list.course.coverImage.thumbnail('coverImg').html({imgAttributes:{class:'img-fluid w-100'}}) | raw }}
                            </div>
                            <div class="new-box__bottom">
                                <div class="new-box__bottom__top">
                                    <div class="categories-tag">{{ list.course.academyType }}</div>
                                    <h3 class="new-box-title m-t-10 m-b-30">{{ list.course.title }}</h3>
                                </div>
                                <div class="new-box__bottom__bottom">
                                    <a href="{{ list.course }}_{{ list.course.getId() }}">Find out more</a>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                </div>
            </div>
        </div>
    </section>
    {{ pimcore_areablock('home-about',{
        "allowed" : [
            "home-page-about"
        ]
    }) }}
