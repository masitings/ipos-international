                                        <script>
                                            function initMap() {
                                                var map = new google.maps.Map(document.getElementById('googleMap'), {
                                                    center: { lat: -34.397, lng: 150.644 }, // Default location if geocoding fails
                                                    zoom: 18
                                                });

                                                // Geocoding service
                                                var geocoder = new google.maps.Geocoder();

                                                // Replace 'Your Location Name' with the location you want to search
                                                var locationName = 'IPOS International, Singapore';
                                                // Geocode the location name to get the coordinates
                                                geocoder.geocode({ 'address': locationName }, function(results, status) {
                                                    if (status === 'OK') {
                                                        // Center the map on the location
                                                        map.setCenter(results[0].geometry.location);

                                                        // Add a marker at the geocoded location
                                                        var marker = new google.maps.Marker({
                                                            map: map,
                                                            position: results[0].geometry.location,
                                                            title: locationName // Tooltip text when hovering over the marker
                                                        });
                                                    } else {
                                                        alert('Geocode was not successful for the following reason: ' + status);
                                                    }
                                                });
                                            }
                                        </script>
