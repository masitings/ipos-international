     <script>
     $(document).ready(function() {
        $('#TNC').change(function () {
            if ($('#TNC').is(':checked')) {
                $('#TNC').val('1');
            } else {
                $('#TNC').val('');
            }
        })

        $('#subsEmail').change(function () {
            if ($('#subsEmail').is(':checked')) {
                $('#subsEmail').val('1');
            } else {
                $('#subsEmail').val('');
            }
        })

        $('.closeBrowsing').click(function () {
            location.reload();
        });
        
        $('#my_captcha_form').on('submit', function(event) {
            var isValid = true;
            $('.validity').each(function (e) {
                var input = $(this);
                console.log('Name : '+input.attr('name')+' | Value : '+input.val());
                if (input.val() === '') {
                    isValid = false;
                }
            });
            if (isValid) {
                $('#submitContact').attr('disabled', true);
                $('#submitContact').text('Loading...');
                var hasClass = $('.thankyou-modal').hasClass('active');
                if (hasClass) {
                    $('#submitContact').attr('disabled', false);
                    $('#submitContact').text('Send');
                }
            }
        });
    });
        
