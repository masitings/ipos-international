{
  "name": "pimcore/asset-metadata-class-definitions",
  "license": "GPL-3.0-or-later",
  "description": "Adds Enterprise Metadata to Pimcore with data object like editor and additional data types for asset metadata.",
  "type": "pimcore-bundle",
  "config": {
    "sort-packages": true
  },
  "require": {
    "pimcore/enterprise-subscription-tools": "^1.0",
    "pimcore/pimcore": "^6.9 || ^10.0",
    "symfony/config": "^4.3 || ^5.2.0",
    "symfony/console": "^4.3 || ^5.2.0",
    "symfony/dependency-injection": "^4.3 || ^5.2.0",
    "symfony/event-dispatcher": "^4.3 || ^5.2.0",
    "symfony/event-dispatcher-contracts": "^2.2",
    "symfony/framework-bundle": "^4.3 || ^5.2.0",
    "symfony/http-foundation": "^4.3 || ^5.2.0",
    "symfony/routing": "^4.3 || ^5.2.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\AssetMetadataClassDefinitionsBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\AssetMetadataClassDefinitionsBundle\\PimcoreAssetMetadataClassDefinitionsBundle"
      ]
    }
  }
}
